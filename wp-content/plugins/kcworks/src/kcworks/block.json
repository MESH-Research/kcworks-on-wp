{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "mesh-research/kcworks",
	"version": "0.1.0",
	"title": "KCWorks",
	"category": "widgets",
	"icon": "list-view",
	"description": "Display results of queries from KCWorks",
	"example": {},
	"attributes": {
		"kcworksQuery": {
			"type": "string"
		},
		"verifiedKcworksQuery": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "kcworks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
