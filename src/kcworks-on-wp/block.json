{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "mesh-research/kcworks-on-wp",
	"version": "1.0.3",
	"title": "KCWorks on WP",
	"category": "widgets",
	"icon": "list-view",
	"description": "Display bibliographies from KCWorks on pages, posts, and sites.",
	"example": {},
	"attributes": {
		"kcworksQuery": {
			"type": "string",
			"default": ""
		},
		"validatedKcworksQuery": {
			"type": "boolean",
			"default": false
		},
		"groupingEnabled": {
			"type": "boolean",
			"default": false
		},
		"citationFormat": {
			"type": "string",
			"default": "apa"
		},
		"headingLevel": {
			"type": "number",
			"default": 2
		}
	},
	"supports": {
		"color": {
			"background": true,
			"text": true,
			"heading": true
		},
		"heading": {
			"typography": {
				"fontSize": true,
				"lineHeight": true
			}
		},
		"html": false,
		"typography": {
			"fontSize": true,
			"lineHeight": true
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"blockGap": true
		}
	},
	"textdomain": "kcworks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js",
	"viewStyle": "file:./view.css"
}
